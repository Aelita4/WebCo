<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/style.css">
    <link rel="icon shortcut" href="/favicon.ico">
    <title>WebCo</title>
</head>
<body>
    <%- include("../../partials/navbar.ejs", { loggedIn: true }); %>
    <%- include("../../partials/resourcebar.ejs", { resources }); %>
    <h2>Hello <%= user %>, you are on <%= planets.find(planet => planet.owner.id === userId).name %> <a onclick="changePlanetName('<%= userId %>')">(change)</a></h2>
    <p>Your buildings:</p>
    <ul id="buildings">
        <% for(const [name, amount] of Object.entries(buildings)) { %>
        <li><span id="building-name-<%= name %>"><%= name %>: </span><span id="building-amount-<%= name %>" amount=<%= amount %>>fetching...</span></li>
        <% } %>
    </ul>
    <script src="/js/game.js"></script>
</body>
</html>